#include <iostream>
using namespace std;
int main()
{ 
   //create a program that will display if the given number is positive or negative and divisible by 2 and 5
    int num;
    cout << "Enter a number: ";
    cin >> num;
    if (num > 0)
    {
       // cout << "The number is positive and divisible by 2 and 5" << endl;
        if ((num % 2 == 0) && (num % 5 == 0))
        {
            cout << "The number is divisible by 2" << endl;
        }
        else
        {
            cout << "The number is not divisible by 2 and 5" << endl;
        }
        if (num % 5 == 0)
        {
            cout << "The number is divisible by 5" << endl;
        }
        else
        {
            cout << "The number is not divisible by 5" << endl;
        }
    }
    else
    {
        cout << "The number is negative" << endl;
    }




}
